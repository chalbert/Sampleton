#stage
  size 100%
  position fixed

#frame
  width 100%
  height 100%
  position absolute
  z-index 0

  div
    position absolute

  .left, .right
    size 100px 100%

  .top, .bottom
    size 100% 100px

  .left
    left -100px
  .right
    right -100px
  .top
    top -100px
  .bottom
    bottom -100px

#boot
  absolute left 50% top -100px
  margin-left -180px
  background url(/images/gumboot-logo.png)
  size 49px 68px
  background-size 100%
